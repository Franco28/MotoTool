<?xml version="1.0" encoding="utf-8"?>
<flashdoha>
<devices>devices</devices> 
<set_a>set_active a</set_a>
<getvar>getvar max-sparse-size</getvar>
<oem>oem fb_mode_set</oem>
<gpt>flash partition gpt.bin</gpt>
<bootloader>flash bootloader bootloader.img</bootloader>
<vbmeta_a>flash vbmeta_a vbmeta.img</vbmeta_a>
<vbmeta_b>flash vbmeta_b vbmeta.img</vbmeta_b>
<modem_a>flash modem_a NON-HLOS.bin</modem_a>
<modem_b>flash modem_b NON-HLOS.bin</modem_b>
<fsg_a>flash fsg_a fsg.mbn</fsg_a>
<fsg_b>flash fsg_b fsg.mbn</fsg_b>
<modemst1>erase modemst1</modemst1>
<modemst2>erase modemst2</modemst2>
<bluetooth_a>flash bluetooth_a BTFM.bin</bluetooth_a>
<bluetooth_b>flash bluetooth_b BTFM.bin</bluetooth_b>
<dsp_a>flash dsp_a dspso.bin</dsp_a>
<dsp_b>flash dsp_b dspso.bin</dsp_b>
<logo_a>flash logo_a logo.bin</logo_a>
<logo_b>flash logo_b logo.bin</logo_b>
<boot_a>flash boot_a boot.img</boot_a>
<boot_b>flash boot_b boot.img</boot_b>
<dtbo_a>flash dtbo_a dtbo.img</dtbo_a>
<dtbo_b>flash dtbo_b dtbo.img</dtbo_b>
<system_a_0>flash system_a system.img_sparsechunk.0</system_a_0>
<system_a_1>flash system_a system.img_sparsechunk.1</system_a_1>
<system_a_2>flash system_a system.img_sparsechunk.2</system_a_2>
<system_a_3>flash system_a system.img_sparsechunk.3</system_a_3>
<system_a_4>flash system_a system.img_sparsechunk.4</system_a_4>
<system_a_5>flash system_a system.img_sparsechunk.5</system_a_5>
<system_b_0>flash system_b system_b.img_sparsechunk.0</system_b_0>
<system_b_1>flash system_b system_b.img_sparsechunk.1</system_b_1>
<oem_a>flash oem_a oem.img</oem_a>
<oem_b>flash oem_b oem_other.img</oem_b>
<vendor_a>flash vendor_a vendor.img</vendor_a>
<vendor_b>flash vendor_b vendor.img</vendor_b>
<erasecarrier>erase carrier</erasecarrier>
<eraseuserdata>erase userdata</eraseuserdata>
<eraseddr>erase ddr</eraseddr>
<oemclear>oem fb_mode_clear</oemclear>
<reboot>reboot</reboot>
</flashdoha>