<?xml version="1.0" encoding="utf-8"?>
<flashpotter>
<devices>devices</devices> 
<getvar>getvar max-sparse-size</getvar>
<oem>oem fb_mode_set</oem>
<gpt>flash partition gpt.bin</gpt>
<bootloader>flash bootloader bootloader.img</bootloader>
<modem_a>flash modem NON-HLOS.bin</modem_a>
<fsg_a>flash fsg fsg.mbn</fsg_a>
<modemst1>erase modemst1</modemst1>
<modemst2>erase modemst2</modemst2>
<bluetooth_a>flash bluetooth BTFM.bin</bluetooth_a>
<dsp_a>flash dsp dspso.bin</dsp_a>
<logo_a>flash logo logo.bin</logo_a>
<boot_a>flash boot boot.img</boot_a>
<system_a_0>flash system system.img_sparsechunk.0</system_a_0>
<system_a_1>flash system system.img_sparsechunk.1</system_a_1>
<system_a_2>flash system system.img_sparsechunk.2</system_a_2>
<system_a_3>flash system system.img_sparsechunk.3</system_a_3>
<system_a_4>flash system system.img_sparsechunk.4</system_a_4>
<system_a_5>flash system system.img_sparsechunk.5</system_a_5>
<system_a_6>flash system system.img_sparsechunk.6</system_a_6>
<system_a_7>flash system system.img_sparsechunk.7</system_a_7>
<system_a_8>flash system system.img_sparsechunk.8</system_a_8>
<oem_a>flash oem oem.img</oem_a>
<erasecarrier>erase carrier</erasecarrier>
<eraseuserdata>erase userdata</eraseuserdata>
<eraseddr>erase ddr</eraseddr>
<oemclear>oem fb_mode_clear</oemclear>
<reboot>reboot</reboot>
</flashpotter>